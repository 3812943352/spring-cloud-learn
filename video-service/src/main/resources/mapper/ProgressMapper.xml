<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.learning.videoservice.mapper.ProgressMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.learning.videoservice.entity.ProgressEntity">
        <id column="ID" property="id"/>
        <result column="user" property="user"/>
        <result column="video" property="video"/>
        <result column="progress" property="progress"/>
        <result column="isDone" property="isDone"/>
        <result column="created" property="created"/>
        <result column="updated" property="updated"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, user, video, progress, isDone, created, updated
    </sql>

</mapper>
