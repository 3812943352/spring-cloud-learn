<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.learning.apisupervisionservice.mapper.IpMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.learning.apisupervisionservice.entity.IpEntity">
        <id column="ID" property="id"/>
        <result column="ip" property="ip"/>
        <result column="time" property="time"/>
        <result column="reason" property="reason"/>
        <result column="bantime" property="bantime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, ip, time, reason, bantime
    </sql>

</mapper>
