<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.learning.courseservice.mapper.ExpertMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.learning.courseservice.entity.ExpertEntity">
        <id column="ID" property="id"/>
        <result column="name" property="name"/>
        <result column="cv" property="cv"/>
        <result column="des" property="des"/>
        <result column="art" property="art"/>
        <result column="img" property="img"/>
        <result column="created" property="created"/>
        <result column="updated" property="updated"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, name, cv, des,art,img,created,updated
    </sql>

</mapper>
